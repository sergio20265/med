{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "schematics": {
    "@schematics/angular:component": {
      "style": "scss"
    }
  },
  "projects": {
    "stacionar-site": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "allowedCommonJsDependencies": [
              "lodash",
              "russian-nouns-js"
            ],
            "outputPath": "dist/stacionar-site/browser",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets",
              "src/robots.txt",
              "src/sitemap.xml"
            ],
            "styles": [
              "src/assets/css/bootstrap.min.css",
              "src/assets/css/fontawesome.min.css",
              "src/assets/css/layerslider.min.css",
              "src/assets/css/jquery.datetimepicker.min.css",
              "src/assets/css/magnific-popup.min.css",
              "src/assets/css/slick.min.css",
              "src/assets/css/animate.min.css",
              "node_modules/primeng/resources/themes/lara-light-blue/theme.css",
              "node_modules/primeng/resources/primeng.min.css",
              "node_modules/ngx-owl-carousel-o/lib/styles/prebuilt-themes/owl.carousel.min.css",
              "node_modules/ngx-owl-carousel-o/lib/styles/prebuilt-themes/owl.theme.default.min.css",
              "src/styles.scss"
            ],
            "scripts": [
              "src/assets/js/vendor/jquery-3.6.0.min.js",
              "src/assets/js/slick.min.js",
              "src/assets/js/layerslider.utils.js",
              "src/assets/js/layerslider.transitions.js",
              "src/assets/js/layerslider.kreaturamedia.jquery.js",
              "src/assets/js/bootstrap.min.js",
              "src/assets/js/jquery.datetimepicker.min.js",
              "src/assets/js/jquery.magnific-popup.min.js",
              "src/assets/js/imagesloaded.pkgd.min.js",
              "src/assets/js/isotope.pkgd.min.js",
              "node_modules/universal-parallax/dist/universal-parallax.min.js",
              "src/assets/js/wow.min.js",
              "src/assets/js/vscustom-carousel.min.js",
              "src/assets/js/ajax-mail.js",
              "src/assets/js/main-core.js"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "3mb",
                  "maximumError": "6mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "25kb",
                  "maximumError": "50kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/app/environments/environment.ts",
                  "with": "src/app/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all",
              "optimization": {
                "scripts": true,
                "styles": {
                  "minify": true,
                  "inlineCritical": true
                },
                "fonts": true
              },
              "buildOptimizer": true,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "namedChunks": false,
              "sourceMap": false,
              "statsJson": false
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "stacionar-site:build:production"
            },
            "development": {
              "browserTarget": "stacionar-site:build:development"
            }
          },
          "defaultConfiguration": "development",
          "options": {
            "port": 4200
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "stacionar-site:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "tsconfig.spec.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": []
          }
        },
        "server": {
          "builder": "@angular-devkit/build-angular:server",
          "options": {
            "outputPath": "dist/stacionar-site/server",
            "main": "server.ts",
            "tsConfig": "tsconfig.server.json"
          },
          "configurations": {
            "production": {
              "outputHashing": "media",
              "fileReplacements": [
                {
                  "replace": "src/app/environments/environment.ts",
                  "with": "src/app/environments/environment.prod.ts"
                }
              ]
            },
            "development": {
              "optimization": false,
              "sourceMap": true,
              "extractLicenses": false,
              "vendorChunk": true,
              "buildOptimizer": false
            }
          },
          "defaultConfiguration": "production"
        },
        "serve-ssr": {
          "builder": "@nguniversal/builders:ssr-dev-server",
          "configurations": {
            "development": {
              "browserTarget": "stacionar-site:build:development",
              "serverTarget": "stacionar-site:server:development"
            },
            "production": {
              "browserTarget": "stacionar-site:build:production",
              "serverTarget": "stacionar-site:server:production"
            }
          },
          "defaultConfiguration": "development"
        },
        "prerender": {
          "builder": "@nguniversal/builders:prerender",
          "options": {
            "routes": [
              "/",
              "/contact",
              "/service",
              "/price",
              "/staff",
              "/diagnos",
              "/rehabilitation",
              "/video-reviews",
              "/license",
              "/transport",
              "/patient/raczion",
              "/error"
            ]
          },
          "configurations": {
            "production": {
              "browserTarget": "stacionar-site:build:production",
              "serverTarget": "stacionar-site:server:production"
            },
            "development": {
              "browserTarget": "stacionar-site:build:development",
              "serverTarget": "stacionar-site:server:development"
            }
          },
          "defaultConfiguration": "production"
        }
      }
    }
  },
  "cli": {
    "analytics": "5e63d5f4-c62b-40aa-b735-b0b65ce0b145"
  }
}
