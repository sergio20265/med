{
  "name": "stacionar-site",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "dev": "ng dev",
    "build": "ng build",
    "preview": "ng preview",
    "serve": "ng serve",
    "test": "ng test",
    "dev:ssr": "ng run stacionar-site:serve-ssr",
    "serve:ssr": "node dist/stacionar-site/server/main.js",
    "build:ssr": "ng build && ng run stacionar-site:server",
    "prerender": "ng run stacionar-site:prerender",
    "generate:sitemap": "node -r dotenv/config scripts/update-sitemap.js",
    "build:full": "npm run generate:sitemap && npm run build:ssr && npm run prerender",
    "build:prod": "npm run generate:sitemap && ng build --configuration production && ng run stacionar-site:server:production && ng run stacionar-site:prerender:production"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.12",
    "@angular/common": "^16.2.12",
    "@angular/compiler": "^16.2.12",
    "@angular/core": "^16.2.12",
    "@angular/forms": "^16.2.12",
    "@angular/platform-browser": "^16.2.12",
    "@angular/platform-browser-dynamic": "^16.2.12",
    "@angular/platform-server": "^16.2.12",
    "@angular/router": "^16.2.12",
    "@nguniversal/express-engine": "^16.2.0",
    "dotenv": "^16.4.5",
    "express": "^4.15.2",
    "http-proxy-middleware": "^2.0.6",
    "lodash": "^4.17.21",
    "ng-yandex-metrika": "^2.2.1",
    "ngx-owl-carousel-o": "^16.0.0",
    "primeng": "^16.9.1",
    "rxjs": "~7.8.0",
    "sitemap": "^8.0.0",
    "tslib": "^2.3.0",
    "universal-parallax": "^1.3.2",
    "zone.js": "~0.13.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.10",
    "@angular/cli": "~16.2.10",
    "@angular/compiler-cli": "^16.2.12",
    "@nguniversal/builders": "^16.2.0",
    "@types/express": "^4.17.0",
    "@types/lodash": "^4.17.20",
    "@types/node": "^18.0.0",
    "browserslist": "^4.21.0",
    "typescript": "~5.1.0"
  }
}
